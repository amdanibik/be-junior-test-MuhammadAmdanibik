                  },
                  "language": {
                    "type": "string"
                  },
                  "status": {
                    "type": "string"
                  },
                  "rating": {
                    "type": "number",
                    "minimum": 1,
                    "maximum": 5
                  }
                }
              },
              "examples": {
                "Avenger": {
                  "description": "Example create data movie",
                  "value": {
                    "name": "Avenger",
                    "language": "English",
                    "status": "started",
                    "rating": 5
                  }
                },
                "X-Men": {
                  "description": "Example create data movie",
                  "value": {
                    "name": "X-Men",
                    "language": "English",
                    "status": "ended",
                    "rating": 2
                  }
                }
              }
            }
          }
        },
